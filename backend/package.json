{
  "name": "products-backend",
  "version": "1.0.0",
  "description": "Backend para sistema de indexaci√≥n de productos con MongoDB y Redis",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "create-indexes": "tsx src/scripts/createIndexes.ts",
    "seed": "tsx src/scripts/seed.ts",
    "docker:start": "bash docker-start.sh",
    "docker:stop": "bash docker-stop.sh",
    "docker:clean": "bash docker-clean.sh",
    "docker:logs": "docker-compose logs -f",
    "setup": "npm run docker:start && sleep 5 && npm run create-indexes && npm run seed"
  },
